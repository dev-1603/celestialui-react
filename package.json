{
	"name": "@celestial-ui/react",
	"version": "1.0.1",
	"description": "A comprehensive React component library with multi-framework styling support",
	"type": "module",
	"license": "MIT",
	"author": {
		"name": "CelestialUI Team",
		"email": "support@celestialui.dev"
	},
	"homepage": "https://github.com/dev-1603/celestialui-react#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/dev-1603/celestialui-react.git"
	},
	"bugs": {
		"url": "https://github.com/dev-1603/celestialui-react/issues"
	},
	"keywords": [
		"react",
		"component-library",
		"ui-components",
		"tailwind",
		"scss",
		"material-design",
		"typescript",
		"storybook"
	],
	"files": [
		"dist",
		"lib"
	],
	"main": "./dist/celestial-ui.umd.js",
	"module": "./dist/celestial-ui.es.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/celestial-ui.es.js",
			"require": "./dist/celestial-ui.umd.js",
			"types": "./dist/index.d.ts"
		},
		"./style.css": "./dist/style.css",
		"./themes/*": "./dist/themes/*",
		"./components/*": "./lib/components/*"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check build-lib build-demo",
		"build-lib": "vite build",
		"build-demo": "vite build --config vite.config.ts",
		"preview": "vite preview",
		"test:unit": "vitest",
		"test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
		"test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
		"test:coverage": "vitest run --coverage",
		"type-check": "tsc --build --force",
		"lint": "eslint .",
		"format": "prettier --write src/ lib/",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"prepublishOnly": "npm run build-lib",
		"release": "npm run build-lib && npm publish",
		"release:beta": "npm run build-lib && npm publish --tag beta",
		"version:patch": "npm version patch",
		"version:minor": "npm version minor",
		"version:major": "npm version major"
	},
	"peerDependencies": {
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@storybook/addon-a11y": "8.5.0",
		"@storybook/addon-docs": "8.5.0",
		"@storybook/addon-essentials": "^8.4.2",
		"@storybook/addon-interactions": "^8.4.2",
		"@storybook/blocks": "^8.4.2",
		"@storybook/react": "^8.4.2",
		"@storybook/react-vite": "^8.4.2",
		"@storybook/test": "^8.4.2",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.0.1",
		"@testing-library/user-event": "^14.5.2",
		"@types/node": "^22.9.0",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@vitejs/plugin-react": "^4.3.3",
		"@vitest/coverage-v8": "^2.1.4",
		"autoprefixer": "^10.4.20",
		"cypress": "^13.15.1",
		"eslint": "^9.13.0",
		"eslint-plugin-react-hooks": "^5.0.0",
		"eslint-plugin-react-refresh": "^0.4.14",
		"globals": "^15.11.0",
		"jsdom": "^25.0.1",
		"npm-run-all2": "^7.0.1",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"sass": "^1.80.6",
		"start-server-and-test": "^2.0.8",
		"storybook": "^8.4.2",
		"tailwindcss": "^3.4.14",
		"typescript": "~5.6.2",
		"typescript-eslint": "^8.11.0",
		"vite": "^5.4.10",
		"vite-plugin-dts": "^4.3.0",
		"vitest": "^2.1.4"
	}
}
